{
  "name": "sushi-react-bootstrap",
  "private": false,
  "version": "0.1.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/sushi-react-bootstrap.umd.js",
  "module": "./dist/sushi-react-bootstrap.es.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/sushi-react-bootstrap.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/sushi-react-bootstrap.es.js",
      "require": "./dist/sushi-react-bootstrap.umd.js"
    },
    "./style.css": "./dist/sushi-react-bootstrap.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:types": "tsc",
    "build:watch": "vite build --watch",
    "lint": "eslint .",
    "preview": "vite preview",
    "prebuild": "rm -rf dist",
    "prepare": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "create-component": "node scripts/create-component.js",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.0.0",
    "react-router": "^7.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.19.0",
    "@rollup/wasm-node": "^4.34.8",
    "@storybook/addon-essentials": "^8.5.6",
    "@storybook/addon-interactions": "^8.5.6",
    "@storybook/addon-onboarding": "^8.5.6",
    "@storybook/blocks": "^8.5.6",
    "@storybook/react": "^8.5.6",
    "@storybook/react-vite": "^8.5.6",
    "@storybook/test": "^8.5.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/lodash": "^4.17.15",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-storybook": "^0.11.3",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "sass": "^1.85.0",
    "sass-embedded": "1.77.5",
    "storybook": "^8.5.6",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "bootstrap",
    "design-system",
    "components",
    "ui-library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiui/sushi-react-bootstrap"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
