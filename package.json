{
  "name": "sushi-react-bootstrap",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/sushi-react-bootstrap.umd.js",
  "module": "./dist/sushi-react-bootstrap.es.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/sushi-react-bootstrap.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/sushi-react-bootstrap.es.js",
      "require": "./dist/sushi-react-bootstrap.umd.js"
    },
    "./style.css": "./dist/sushi-react-bootstrap.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run prebuild && tsc && vite build",
    "build:types": "tsc",
    "build:watch": "vite build --watch",
    "lint": "eslint .",
    "preview": "vite preview",
    "prebuild": "rm -rf dist",
    "prepare": "npm run build"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "sass": "^1.85.0",
    "sass-embedded": "1.77.5",  
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "bootstrap",
    "design-system",
    "components",
    "ui-library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiui/sushi-react-bootstrap"
  }
}
